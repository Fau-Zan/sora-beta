{
  "name": "violet",
  "version": "1.0.3",
  "description": "A Whatsapp Bot",
  "type": "module",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "prepack": "yarn install",
    "build": "tsc && tsc-alias",
    "format": "prettier 'script/**/*.{ts,js,json}' --write",
    "clean": "rimraf ./lib",
    "debug": "yarn run build && node --experimental-loader=extensionless --no-warnings debug.js",
    "start": "yarn build && node --experimental-loader=extensionless --no-warnings lib/index.js 2>&1 || true",
    "start:dev": "yarn run clean && yarn start",
    "start:pm2": "pm2 start .pm2-process.json",
    "stop:pm2": "pm2 stop .pm2-process.json",
    "delete-wa-session": "node delete-wa-session.cjs",
    "delete-wa-session:dry": "node delete-wa-session.cjs --dry-run",
    "test": "yarn build && mocha \"./lib/test/**/*.spec.js\"",
    "test:watch": "yarn build && mocha --watch \"./lib/test/**/*.spec.js\""
  },
  "keywords": [],
  "author": "Fauzan <fzn@yahoo.id>",
  "license": "ISC",
  "dependencies": {
    "@adiwajshing/keyed-db": "^0.2.4",
    "@hapi/boom": "^10.0.0",
    "@rifandavinci/ai-chatbot": "^1.1.0",
    "@whiskeysockets/baileys": "^7.0.0-rc.9",
    "ansi": "^0.3.1",
    "axios": "1.4.0",
    "chalk": "^5.2.0",
    "cheerio": "^1.0.0-rc.12",
    "cycletls": "^1.0.22",
    "dotenv": "^16.0.3",
    "envalid": "^8.0.0",
    "file-type": "^18.2.1",
    "fluent-ffmpeg": "^2.1.2",
    "form-data": "^4.0.0",
    "groq-sdk": "^0.27.0",
    "jimp": "^0.22.8",
    "libphonenumber-js": "^1.10.15",
    "link-preview-js": "^3.0.4",
    "lodash": "^4.17.21",
    "micromatch": "^4.0.5",
    "mongodb": "^6.19.0",
    "node-cache": "^5.1.2",
    "pg": "^8.11.3",
    "pino": "^8.8.0",
    "pino-pretty": "^10.3.1",
    "pouchdb": "^8.0.0",
    "pouchdb-find": "^8.0.1",
    "pouchdb-upsert": "^2.2.0",
    "puppeteer": "^24.12.1",
    "qrcode": "^1.5.4",
    "qrcode-terminal": "^0.12.0",
    "similarity": "^1.2.1",
    "tmp-promise": "^3.0.3",
    "uuid": "^9.0.0",
    "wa-sticker-formatter": "^4.4.4",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.11.17",
    "@types/pouchdb": "^6.4.0",
    "@types/ws": "^8.5.10",
    "extensionless": "^1.9.6",
    "prettier": "^2.8.4",
    "rimraf": "^5.0.1",
    "tsc-alias": "^1.8.2",
    "typescript": "^4.9.4"
  },
  "extensionless": {
    "lookFor": [
      "js",
      "json"
    ],
    "resolveDirs": true
  },
  "files": [
    "lib"
  ],
  "engines": {
    "node": ">=16",
    "npm": ">=7"
  },
  "repository": "https://github.com/Fau-Zan/violet"
}
